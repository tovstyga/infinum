source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '10.0'
use_frameworks!

inhibit_all_warnings!

def standart_pods

  pod 'Alamofire', '~> 4.7.3'
  pod 'Kingfisher', '~> 4.10.0'
  pod 'ObjectMapper', '~> 3.3.0'

end

target 'TVShows' do

  standart_pods
  
end

target 'TVShows (Dev)' do

  standart_pods
  
end
